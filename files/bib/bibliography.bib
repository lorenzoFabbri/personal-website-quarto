  keywords = {/unread,Alveolar Bone Loss,Analysis of Variance,{Data Interpretation, Statistical},Dental Research,Gingival Recession,{Guided Tissue Regeneration, Periodontal},Humans,Mathematics,{Outcome Assessment, Health Care},Read,Regression Analysis,Research Design},
