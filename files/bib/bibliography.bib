}

@article{TuWoolstonBaxter:2010,
  title = {Assessing the {{Impact}} of {{Body Size}} in {{Childhood}} and {{Adolescence}} on {{Blood Pressure}}: {{An Application}} of {{Partial Least Squares Regression}}},
  shorttitle = {Assessing the {{Impact}} of {{Body Size}} in {{Childhood}} and {{Adolescence}} on {{Blood Pressure}}},
  author = {Tu, Yu-Kang and Woolston, Andrew and Baxter, Paul D. and Gilthorpe, Mark S.},
  date = {2010-07},
  journaltitle = {Epidemiology},
  volume = {21},
  number = {4},
