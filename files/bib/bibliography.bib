@article{GruberLeePhillips:2022,
  title = {Developing a {{Targeted Learning-Based Statistical Analysis Plan}}},
  author = {Gruber, Susan and Lee, Hana and Phillips, Rachael and Ho, Martin and family=Laan, given=Mark, prefix=van der, useprefix=true},
