@article{LeeKarvonen-GutierrezMukherjee:2022,
  title = {Race-Specific Associations of Urinary Phenols and Parabens with Adipokines in Midlife Women: {{The Study}} of {{Women}}'s {{Health Across}} the {{Nation}} ({{SWAN}})},
  shorttitle = {Race-Specific Associations of Urinary Phenols and Parabens with Adipokines in Midlife Women},
  author = {Lee, Seulbi and Karvonen-Gutierrez, Carrie and Mukherjee, Bhramar and Herman, William H. and Park, Sung Kyun},
  date = {2022-06-15},
  journaltitle = {Environmental Pollution},
  shortjournal = {Environmental Pollution},
