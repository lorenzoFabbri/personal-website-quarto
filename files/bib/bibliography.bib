@article{FarrellWergeSklar:2015,
  title = {Evaluating Historical Candidate Genes for Schizophrenia},
  author = {Farrell, M. S. and Werge, T. and Sklar, P. and Owen, M. J. and Ophoff, R. A. and O'Donovan, M. C. and Corvin, A. and Cichon, S. and Sullivan, P. F.},
