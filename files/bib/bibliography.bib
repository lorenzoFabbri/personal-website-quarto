@article{VrijheidBasaganaGonzalez:2021,
  title = {Advancing Tools for Human Early Lifecourse Exposome Research and Translation ({{ATHLETE}}): {{Project}} Overview},
  shorttitle = {Advancing Tools for Human Early Lifecourse Exposome Research and Translation ({{ATHLETE}})},
  author = {Vrijheid, Martine and Basagaña, Xavier and Gonzalez, Juan R. and Jaddoe, Vincent W. V. and Jensen, Genon and Keun, Hector C. and McEachan, Rosemary R. C. and Porcel, Joana and Siroux, Valerie and Swertz, Morris A. and Thomsen, Cathrine and Aasvang, Gunn Marit and Andrušaitytė, Sandra and Angeli, Karine and Avraam, Demetris and Ballester, Ferran and Burton, Paul and Bustamante, Mariona and Casas, Maribel and Chatzi, Leda and Chevrier, Cécile and Cingotti, Natacha and Conti, David and Crépet, Amélie and Dadvand, Payam and Duijts, Liesbeth and family=Enckevort, given=Esther, prefix=van, useprefix=true and Esplugues, Ana and Fossati, Serena and Garlantezec, Ronan and Gómez Roig, María Dolores and Grazuleviciene, Regina and Gützkow, Kristine B. and Guxens, Mònica and Haakma, Sido and Hessel, Ellen V. S. and Hoyles, Lesley and Hyde, Eleanor and Klanova, Jana and family=Klaveren, given=Jacob D., prefix=van, useprefix=true and Kortenkamp, Andreas and Le Brusquet, Laurent and Leenen, Ivonne and Lertxundi, Aitana and Lertxundi, Nerea and Lionis, Christos and Llop, Sabrina and Lopez-Espinosa, Maria-Jose and Lyon-Caen, Sarah and Maitre, Lea and Mason, Dan and Mathy, Sandrine and Mazarico, Edurne and Nawrot, Tim and Nieuwenhuijsen, Mark and Ortiz, Rodney and Pedersen, Marie and Perelló, Josep and Pérez-Cruz, Míriam and Philippat, Claire and Piler, Pavel and Pizzi, Costanza and Quentin, Joane and Richiardi, Lorenzo and Rodriguez, Adrian and Roumeliotaki, Theano and Sabin Capote, José Manuel and Santiago, Leonardo and Santos, Susana and Siskos, Alexandros P. and Strandberg-Larsen, Katrine and Stratakis, Nikos and Sunyer, Jordi and Tenenhaus, Arthur and Vafeiadi, Marina and Wilson, Rebecca C. and Wright, John and Yang, Tiffany and Slama, Remy},
  date = {2021-10},
  journaltitle = {Environmental Epidemiology (Philadelphia, Pa.)},
  shortjournal = {Environ Epidemiol},
  volume = {5},
  number = {5},
  eprint = {34934888},
  eprinttype = {pmid},
  pages = {e166},
  issn = {2474-7882},
  doi = {10.1097/EE9.0000000000000166},
  abstract = {Early life stages are vulnerable to environmental hazards and present important windows of opportunity for lifelong disease prevention. This makes early life a relevant starting point for exposome studies. The Advancing Tools for Human Early Lifecourse Exposome Research and Translation (ATHLETE) project aims to develop a toolbox of exposome tools and a Europe-wide exposome cohort that will be used to systematically quantify the effects of a wide range of community- and individual-level environmental risk factors on mental, cardiometabolic, and respiratory health outcomes and associated biological pathways, longitudinally from early pregnancy through to adolescence. Exposome tool and data development include as follows: (1) a findable, accessible, interoperable, reusable (FAIR) data infrastructure for early life exposome cohort data, including 16 prospective birth cohorts in 11 European countries; (2) targeted and nontargeted approaches to measure a wide range of environmental exposures (urban, chemical, physical, behavioral, social); (3) advanced statistical and toxicological strategies to analyze complex multidimensional exposome data; (4) estimation of associations between the exposome and early organ development, health trajectories, and biological (metagenomic, metabolomic, epigenetic, aging, and stress) pathways; (5) intervention strategies to improve early life urban and chemical exposomes, co-produced with local communities; and (6) child health impacts and associated costs related to the exposome. Data, tools, and results will be assembled in an openly accessible toolbox, which will provide great opportunities for researchers, policymakers, and other stakeholders, beyond the duration of the project. ATHLETE's results will help to better understand and prevent health damage from environmental exposures and their mixtures from the earliest parts of the life course onward.},
  langid = {english},
  pmcid = {PMC8683140},
  keywords = {/unread,Adolescent health,Child health,Early life,Exposome,Exposure assessment,To Read},
