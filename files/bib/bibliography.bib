  title = {Early-{{Life Environmental Exposures}} and {{Blood Pressure}} in {{Children}}},
  author = {Warembourg, Charline and Maitre, Léa and Tamayo-Uria, Ibon and Fossati, Serena and Roumeliotaki, Theano and Aasvang, Gunn Marit and Andrusaityte, Sandra and Casas, Maribel and Cequier, Enrique and Chatzi, Lida and Dedele, Audrius and Gonzalez, Juan-Ramon and Gražulevičienė, Regina and Haug, Line Smastuen and Hernandez-Ferrer, Carles and Heude, Barbara and Karachaliou, Marianna and Krog, Norun Hjertager and McEachan, Rosemary and Nieuwenhuijsen, Mark and Petraviciene, Inga and Quentin, Joane and Robinson, Oliver and Sakhi, Amrit Kaur and Slama, Rémy and Thomsen, Cathrine and Urquiza, Jose and Vafeiadi, Marina and West, Jane and Wright, John and Vrijheid, Martine and Basagaña, Xavier},
