@article{HeindelBalbusBirnbaum:2015,
  title = {Developmental {{Origins}} of {{Health}} and {{Disease}}: {{Integrating Environmental Influences}}},
  shorttitle = {Developmental {{Origins}} of {{Health}} and {{Disease}}},
  author = {Heindel, Jerrold J. and Balbus, John and Birnbaum, Linda and Brune-Drisse, Marie Noel and Grandjean, Philippe and Gray, Kimberly and Landrigan, Philip J. and Sly, Peter D. and Suk, William and Slechta, Deborah Cory and Thompson, Claudia and Hanson, Mark},
