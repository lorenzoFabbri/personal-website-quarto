  author = {Spanier, Adam J. and Fausnight, Tracy and Camacho, Tareq F. and Braun, Joseph M.},
  date = {2014-11-01},
  journaltitle = {Allergy and Asthma Proceedings},
  shortjournal = {Allergy and Asthma Proceedings},
  volume = {35},
