  author = {Liao, Sui-Ling and Tsai, Ming-Han and Lai, Shen-Hao and Yao, Tsung-Chieh and Hua, Man-Chin and Yeh, Kuo-Wei and Chiang, Chi-Hsin and Huang, Shih-Yin and Huang, Jing-Long},
