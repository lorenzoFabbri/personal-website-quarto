  title = {Triclosan Inhibition of Acute and Chronic Inflammatory Gene Pathways},
  author = {Barros, Silvana P. and Wirojchanasak, Sodsi and Barrow, David A. and Panagakos, Fotinos S. and Devizio, William and Offenbacher, Steven},
