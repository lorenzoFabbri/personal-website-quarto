@article{KrasnovaTomValeri:2023,
  title = {Direct {{Effect}} of {{Life-Course Socioeconomic Status}} on {{Late-Life Cognition}} and {{Cognitive Decline}} in the {{Rush Memory}} and {{Aging Project}}},
  author = {Krasnova, Anna and Tom, Sarah E and Valeri, Linda and Crane, Paul K and Bennett, David A},
