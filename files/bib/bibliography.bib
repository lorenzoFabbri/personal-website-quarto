@article{GilsanzYoungGlymour:2022,
  title = {Marginal {{Structural Models}} for {{Life-Course Theories}} and {{Social Epidemiology}}: {{Definitions}}, {{Sources}} of {{Bias}}, and {{Simulated Illustrations}}},
