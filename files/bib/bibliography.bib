  author = {Hu, Peipei and Vinturache, Angela and Li, Hong and Tian, Ying and Yuan, Lei and Cai, Chen and Lu, Min and Zhao, Jiuru and Zhang, Qianqian and Gao, Yu and Liu, Zhiwei and Ding, Guodong},
