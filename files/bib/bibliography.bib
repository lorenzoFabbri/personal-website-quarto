  keywords = {/unread,Adipose Tissue,Animals,{Anti-Infective Agents, Local},Biomarkers,Blood Glucose,{Diabetes, Gestational},{Dose-Response Relationship, Drug},Down-Regulation,Female,Gestational Age,gestational diabetes mellitus,Glucose Transporter Type 4,Insulin,insulin resistance,Insulin Resistance,Insulin-Secreting Cells,Male,Maternal Exposure,{Mice, Inbred ICR},{Muscle, Skeletal},Phosphorylation,PPAR gamma,Pregnancy,Proto-Oncogene Proteins c-akt,Signal Transduction,Thyroid Gland,thyroid hormones,Thyroxine,Time Factors,Triclosan,Triiodothyronine},
