  author = {Wnuk, Agnieszka and Rzemieniec, Joanna and Staroń, Jakub and Litwa, Ewa and Lasoń, Władysław and Bojarski, Andrzej and Kajta, Małgorzata},
  date = {2019-07-01},
  journaltitle = {Molecular Neurobiology},
  shortjournal = {Mol Neurobiol},
  volume = {56},
