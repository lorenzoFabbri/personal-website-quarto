  shorttitle = {Short-Term Effect of Air Pollution on Attention Function in Adolescents ({{ATENC}}!{{Ó}})},
  author = {Gignac, Florence and Barrera-Gómez, Jose and Persavento, Cecilia and Solé, Caterina and Tena, Èlia and López-Vicente, Mónica and Foraster, Maria and Amato, Fulvio and Alastuey, Andrés and Querol, Xavier and Llavador, Humberto and Apesteguia, Jose and Júlvez, Jordi and Couso, Digna and Sunyer, Jordi and Basagaña, Xavier},
  date = {2021-11-01},
  journaltitle = {Environment International},
