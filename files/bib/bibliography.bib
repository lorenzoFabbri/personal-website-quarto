}

@article{Hill:2011,
  title = {Bayesian {{Nonparametric Modeling}} for {{Causal Inference}}},
  author = {Hill, Jennifer L.},
  date = {2011-01-01},
  journaltitle = {Journal of Computational and Graphical Statistics},
  volume = {20},
