  author = {Bowden, Jack and Del Greco M, Fabiola and Minelli, Cosetta and Zhao, Qingyuan and Lawlor, Debbie A and Sheehan, Nuala A and Thompson, John and Davey Smith, George},
  date = {2019-06-01},
  journaltitle = {International Journal of Epidemiology},
  shortjournal = {International Journal of Epidemiology},
  volume = {48},
