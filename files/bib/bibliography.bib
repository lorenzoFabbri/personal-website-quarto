@article{BarryLawlorShapland:2022,
  title = {Using {{Mendelian Randomisation}} to {{Prioritise Candidate Maternal Metabolic Traits Influencing Offspring Birthweight}}},
  author = {Barry, Ciarrah-Jane Shannon and Lawlor, Deborah A. and Shapland, Chin Yang and Sanderson, Eleanor and Borges, Maria Carolina},
  date = {2022-06-10},
