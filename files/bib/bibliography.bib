  keywords = {/unread,Adult,Biomarkers,endocrine disorders,Environmental Monitoring,exposure assessment,France,Humans,hygiene / occupational hygiene,Industry,Linear Models,Longitudinal Studies,Male,male reproduction,Middle Aged,Occupational Exposure,occupational health practice,Phthalic Acids,Plastics,Testosterone},
