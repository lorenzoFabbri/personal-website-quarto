@article{KonieckiWangMoody:2011,
  title = {Phthalates in Cosmetic and Personal Care Products: {{Concentrations}} and Possible Dermal Exposure},
  shorttitle = {Phthalates in Cosmetic and Personal Care Products},
  author = {Koniecki, Diane and Wang, Rong and Moody, Richard P. and Zhu, Jiping},
