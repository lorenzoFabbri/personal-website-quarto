@article{HeyerMeredith:2017,
  title = {Environmental Toxicology: {{Sensitive}} Periods of Development and Neurodevelopmental Disorders},
  shorttitle = {Environmental Toxicology},
  author = {Heyer, Djai B. and Meredith, Rhiannon M.},
  date = {2017-01},
