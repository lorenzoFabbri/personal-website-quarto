  author = {Chai, Peng and Tian, Jingzhong and Zhao, Deyin and Zhang, Hongyan and Cui, Jian and Ding, Keshuo and Liu, Bin},
  date = {2016-02-26},
  journaltitle = {Biochemical and Biophysical Research Communications},
  shortjournal = {Biochem Biophys Res Commun},
