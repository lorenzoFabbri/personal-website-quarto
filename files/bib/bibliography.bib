@article{NorrisMansukoskiGilthorpe:2021,
  title = {Early Childhood Weight Gain: {{Latent}} Patterns and Body Composition Outcomes},
  shorttitle = {Early Childhood Weight Gain},
  author = {Norris, Tom and Mansukoski, Liina and Gilthorpe, Mark S. and Hamer, Mark and Hardy, Rebecca and Howe, Laura D. and Li, Leah and Ong, Ken K. and Ploubidis, George B. and Viner, Russell M. and Johnson, William},
  date = {2021},
  journaltitle = {Paediatric and Perinatal Epidemiology},
  volume = {35},
  number = {5},
